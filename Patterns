public class Main{
    public static void main(String[] args){
        pattern4(5);
    }
    static void pattern4(int n){
        for (int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print(j +" ");
            }
            System.out.println();
        }
    }
}

Python:

def pattern4(n):
    for row in range(1,n+1):
        for col in range(1,row+1):
            print(col,end="")
            
        print()
        
pattern4(5)

