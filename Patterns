17)
      1
     212
    32123
   4321234
    32123
     212
      1

Java:
public class Main
{
	public static void main(String[] args) {
		pattern17(4);
	}
	static void pattern17(int n){
	    // upper Half
	    for(int i=1;i<n+1;i++){
	        for(int j=1;j<=n-i;j++){
	            System.out.print(" ");
	        }
	        // left numbers
	        for (int j = i; j >= 1; j--) {
                System.out.print(j);
            }

            // right numbers
            for (int j = 2; j <= i; j++) {
                System.out.print(j);
            }

            System.out.println();
	        
	    }
	    // Lower Half
        for (int i = n - 1; i >= 1; i--) {
            // spaces
            for (int s = 1; s <= n - i; s++) {
                System.out.print(" ");
            }

            // left numbers
            for (int j = i; j >= 1; j--) {
                System.out.print(j);
            }

            // right numbers
            for (int j = 2; j <= i; j++) {
                System.out.print(j);
            }

            System.out.println();
        }
	}
}

Python:
def pattern17(n):
    # upper half
    for i in range(1, n + 1):
        print(" " * (n - i), end="")
        for j in range(i, 0, -1):  # left side
            print(j, end="")
        for j in range(2, i + 1):  # right side
            print(j, end="")
        print()  # newline after each row

    # lower half
    for i in range(n - 1, 0, -1):
        print(" " * (n - i), end="")
        for j in range(i, 0, -1):
            print(j, end="")
        for j in range(2, i + 1):
            print(j, end="")
        print()  # newline after each row

# Call the function
pattern17(4)

