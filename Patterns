Java:
public class StarPattern {
    public static void main(String[] args) {
        int n = 5;

        // Top half
        for (int i = 0; i < n; i++) {
            int stars = n - i;
            int spaces = i * 2;

            for (int j = 0; j < stars; j++) System.out.print("*");
            for (int j = 0; j < spaces; j++) System.out.print(" ");
            for (int j = 0; j < stars; j++) System.out.print("*");

            System.out.println();
        }

        // Bottom half
        for (int i = 0; i < n; i++) {
            int stars = i + 1;
            int spaces = (n - i - 1) * 2;

            for (int j = 0; j < stars; j++) System.out.print("*");
            for (int j = 0; j < spaces; j++) System.out.print(" ");
            for (int j = 0; j < stars; j++) System.out.print("*");

            System.out.println();
        }
    }
}

Python:
def pattern18(n):
    #top half
    for i in range(n):
        stars=n-i
        spaces=i*2
        print("*" * stars + " " * spaces + "*" * stars)
        
    #bottom half
    for i in range(n):
        stars=i+1
        spaces=(n-i-1)*2
        print("*" * stars + " " * spaces + "*" * stars)
        
pattern18(5)

