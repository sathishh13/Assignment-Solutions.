Java:
public class Main{
    public static void main(String[] args){
        pattern15(5);
    }
    static void pattern15(int n){
        for(int i=1;i<=n;i++){
            // spaces
            for(int space=1;space<=n-i;space++){
                System.out.print(" ");
            }
            // for *
            for(int j=1;j<=2*i-1;j++){
                //print * at borders or last row
                if(j==1 || j==2*i-1){
                    System.out.print("*");
                }else{
                    System.out.print(" ");
                }
            }
            System.out.println();
        } 
        for(int i=n-1;i>=1;i--){
            // spaces
            for(int space=1;space<=n-i;space++){
                System.out.print(" ");
            }
            // for printing * and spaces inside 
            for(int j=1;j<=2*i-1;j++){
                //print * at borders or last row
                if(j==1 || j==2*i-1 ){
                    System.out.print("*");
                }else{
                    System.out.print(" ");
                }
            }
            System.out.println();
        }
    }
}
Python:
def pattern15(n):
    for row in range(1,n+1):
        # print leading spaces
        for space in range(n - row):
            print(" ", end="")
        # print stars and inner spaces
        for col in range(1, 2 * row):
            if col == 1 or col == 2 * row - 1 :
                print("*", end="")
            else:
                print(" ", end="")
        print()
    for row in range(n-1,0,-1):
        # print leading spaces
        for space in range(n - row):
            print(" ", end="")
        # print stars and inner spaces
        for col in range(1, 2 * row):
            if col == 1 or col == 2 * row - 1 :
                print("*", end="")
            else:
                print(" ", end="")
        print()


pattern15(5)

